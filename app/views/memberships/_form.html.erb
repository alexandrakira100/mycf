<div class="form-style-5">
  <h1>ADD NEW MEMBERSHIP</h1>
    <%= simple_form_for [ @fund, @membership ] do |f| %>
      <div class="form-group">
        <%= text_field_tag(:email) %>
      </div>
      <div class="form-group">
        <%= f.input :allocation_share %>
      </div>
      <%= f.submit "ADD", class: "btn btn-primary" %>
  <% end %>
</div>
